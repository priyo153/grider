package grider;

public class Stack {
	
	int [] stack;
	int size;
	int top=0;
	
	Stack(int size){
		
		this.size=size;
		
		stack=new int[size];
		
	}
	
	void push(int n) {
		if(top==size) {
			throw new RuntimeException("overflow");
		}
		stack[top]=n;
		top++;
	}
	
	int pop() {
		if(top==0) {
			throw new RuntimeException("underflow");
			
		}
		top--;
		return stack[top];
		
		
	}
	
	public static void main(String[] args) {
		Stack s=new Stack(5);
		s.push(1);
		s.push(2);
		s.push(3);
	
		
		for(int i=0;i<s.size;i++) {
			
			System.out.println(s.pop());
			
		}
	}

}
