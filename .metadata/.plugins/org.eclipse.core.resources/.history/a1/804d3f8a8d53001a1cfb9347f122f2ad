package grider;

public class FindFromTail {
	static Node findFromTail(LinkedList lst,int num) {
		
		Node slow=lst.getFirst();
		Node fast=lst.getFirst();
		
		fast=lst.getAt(num);
		
		while(fast.next!=null) {
			
			fast=fast.next;
			slow=slow.next;
			
		}
		
		return slow;
		
		
	}
	
	public static void main(String[] args) {
		LinkedList lst=new LinkedList();
		lst.insertLast(0);
		lst.insertLast(1);
		lst.insertLast(2);
		lst.insertLast(3);
		lst.insertLast(4);
		lst.insertLast(5);
		lst.insertLast(6);
		lst.insertLast(7);
		lst.insertLast(8);
		
		lst.traverse();
		
		System.out.println(findFromTail(lst,1).data);
		
		
		
	}

}
