package grider;

import java.util.List;

class TreeNode{
	int data;
	List<TreeNode> children;
	
	TreeNode(int val){
		data=val;
	}
	
	void add(int val) {
		children.add(new TreeNode(val));
	}
	
	void remove(int val) {
		children.removeIf(n -> (n.data==val));
	}
}

public class Tree {
	
	
	TreeNode root;
	
	Tree(){
		root=new TreeNode(0);
	}
	
	TreeNode getRoot() {
		return root;
	}

}
