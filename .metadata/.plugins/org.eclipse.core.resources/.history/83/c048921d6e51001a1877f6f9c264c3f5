package grider;

public class Queue {
	
	int queue[];
	int front=0;
	int rear=0;
	int count=0;
	int size;
	
	
	Queue(int size){
		this.size=size;
		queue=new int[size];
	}
	
	public void enqueue(int n) {
		
		if(count==queue.length) {
			throw new RuntimeException("overflow");
			
		}
		
		queue[rear]=n;
		rear=(rear+1)%size;
		
		
		
	}
	
	public int dequeue() {
		
		if(front==rear) {
			
			throw new RuntimeException("underflow");
		}
		
		int res= queue[front];
		front=(front+1)%size;
		return res;
		
		
		
	}
	
	
	
	public static void main(String[] args) {}

}
